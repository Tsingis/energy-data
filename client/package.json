{
  "name": "energy-data",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./config/vite.config.ts --open",
    "audit": "npm audit --audit-level low",
    "lint": "npx eslint . --config ./config/eslint.config.js",
    "lint:fix": "npx eslint . --config ./config/eslint.config.js --fix",
    "prettier": "npx prettier --check '**/*.{js,ts,vue,css,html}'",
    "prettier:fix": "npx prettier --write '**/*.{js,ts,vue,css,html}'",
    "typecheck": "vue-tsc --noEmit -p ./config/tsconfig.json",
    "build": "vue-tsc -b ./config/tsconfig.json && vite --config ./config/vite.config.ts build"
  },
  "dependencies": {
    "chart.js": "^4.4.7",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.19.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite": "^6.0.11",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
