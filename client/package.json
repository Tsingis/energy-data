{
  "name": "energy-data",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config .vite.config.ts --open",
    "dev:check": "vite --config ./vite.config.ts --open -- --use-checker",
    "test": "vitest --config vitest.config.ts",
    "audit": "npm audit --audit-level low",
    "lint": "eslint . --config ./eslint.config.js",
    "lint:fix": "eslint . --config ./eslint.config.js --fix",
    "prettier": "prettier --check '**/*.{js,ts,vue,css,html}'",
    "prettier:fix": "prettier --write '**/*.{js,ts,vue,css,html}'",
    "typecheck": "vue-tsc --noEmit -p ./tsconfig.json",
    "build": "vue-tsc -b ./tsconfig.json && vite --config ./vite.config.ts build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "chart.js": "^4.4.8",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^9.24.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "typescript": "5.8.3",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  }
}
